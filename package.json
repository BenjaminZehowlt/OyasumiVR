    "cookie": "^0.5.0",
    "eslint-plugin-json": "^3.1.0",
    "flag-icons": "^6.6.6",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "marked": "^4.2.2",
    "material-icons": "^1.12.1",
    "moment": "^2.29.4",
    "ngx-pipes": "^3.2.0",
